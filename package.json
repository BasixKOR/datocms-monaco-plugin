{
  "name": "datocms-plugin-monaco",
  "version": "0.0.4",
  "license": "MIT",
  "homepage": "https://github.com/BasixKOR/datocms-monaco-plugin",
  "description": "Monaco editor for DatoCMS",
  "keywords": [
    "code",
    "syntax highlighting",
    "editor",
    "datocms",
    "monaco",
    "datocms-plugin"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepack": "MSYS2_ARG_CONV_EXCL='--base=' yarn build --base='/dist/'"
  },
  "files": [
    "dist/**",
    "docs/*"
  ],
  "dependencies": {
    "@monaco-editor/react": "^4.3.1",
    "datocms-plugin-sdk": "^0.5.2",
    "datocms-react-ui": "^0.5.2",
    "lodash": "^4.17.21",
    "monaco-editor": "^0.32.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.178",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@vitejs/plugin-react": "^1.0.7",
    "typescript": "^4.5.4",
    "vite": "^2.8.0"
  },
  "datoCmsPlugin": {
    "title": "Monaco Editor",
    "previewImage": "docs/preview.png",
    "coverImage": "docs/cover.png",
    "entryPoint": "dist/index.html",
    "permissions": []
  },
  "packageManager": "yarn@3.1.1"
}
